<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>0.0.7</Version>
    <AssemblyVersion>0.0.7</AssemblyVersion>
    <FileVersion>0.0.7</FileVersion>
    <AssemblyTitle>Stain Selector</AssemblyTitle>
    <AssemblyDescription>Portable wood stain formula calculator with auto-update capabilities</AssemblyDescription>
    <AssemblyCompany>StainWare</AssemblyCompany>
    <AssemblyProduct>Stain Selector</AssemblyProduct>
    <AssemblyCopyright>Copyright Â© 2024</AssemblyCopyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="8.0.4" />
    <PackageReference Include="System.IO.Compression" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\deploy_key" />
  </ItemGroup>

</Project>
